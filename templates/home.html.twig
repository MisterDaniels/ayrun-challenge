<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Auryn Challenge!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        {% endblock %}
    </head>
    <body>
        {% block body %}

            <div id="app" class="container">

                <photo-image
                    v-for="(photo, index) in photosReaded"
                    v-bind:photo="photo" 
                    v-bind:key="index">
                </photo-image>
                <button class="btn btn-primary" v-on:click="getPhotosSize();">Carregar Tamanho</button>
                <button class="btn btn-primary" v-on:click="getPhotosData(2);">Carregar</button>

            </div>

        {% endblock %}
        {% block javascripts %}
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
            <script src="https://unpkg.com/axios@0.19.0/dist/axios.min.js"></script>            
            <script src="/js/app.js"></script>
        {% endblock %}
    </body>
</html>